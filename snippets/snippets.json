{
  "arrowFunc": {
    "body": [
      "const $1 = ($2) => $3"
    ],
    "description": "single-line function",
    "prefix": "zfunc"
  },
  "braces": {
    "body": [
      "{$1} $2"
    ],
    "description": "braces",
    "prefix": "zbra"
  },
  "catch": {
    "body": [
      ".catch(err => console.log(err, \"error\"))"
    ],
    "description": "Promise catch",
    "prefix": "zcatch"
  },
  "component": {
    "body": [
      "export const $1 = ({$2, ...props}) => {",
      "  $4",
      "  return (",
      "   $3",
      " )",
      "}"
    ],
    "description": "React Component",
    "prefix": "zcomp"
  },
  "consoleLog": {
    "body": [
      "console.log('$1', $2);"
    ],
    "description": "Log output to console",
    "prefix": "zlog"
  },
  "import": {
    "body": [
      "import {$1} from '$2';"
    ],
    "description": "import statement",
    "prefix": "zimp"
  },
  "parenthesis": {
    "body": [
      "($1) $2"
    ],
    "description": "parens",
    "prefix": "zpar"
  },
  "useEffect": {
    "body": [
      "useEffect(() => {",
      "$1",
      "}, []);"
    ],
    "description": "useState",
    "prefix": "zuseEffect"
  },
  "useState": {
    "body": [
      "const [$1, $2] = useState($3);"
    ],
    "description": "useState",
    "prefix": "zuseState"
  }
}
