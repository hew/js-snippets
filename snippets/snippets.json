{
  "console.log": {
    "prefix": "cl",
    "body": [
      "console.log('$1', $1);"
    ],
    "description": "Log output to console"
  },
  "arrow function": {
    "prefix": "func",
    "body": [
      "($1) => $2"
    ],
    "description": "single-line function"
  },
  "braces": {
    "prefix": "br",
    "body": [
      "{$1} $2"
    ],
    "description": "braces"
  },
  "parens": {
    "prefix": "par",
    "body": [
      "($1) $2"
    ],
    "description": "braces"
  },
  "import": {
    "prefix": "imp",
    "body": [
      "import {$1} from '$2';"
    ],
    "description": "import statement"
  },
  "catch": {
    "prefix": "ct",
    "body": [
      ".catch(err => console.log(err, \"error\"))"
    ],
    "description": "Promise catch"
  },
  "component": {
    "prefix": "comp",
    "body": [
      "export const $1 = ({$2,...props}) => {",
      "  $4",
      "  return (",
      "    $3",
        ")",
      "}"
    ],
    "description": "React Component"
  }
}
