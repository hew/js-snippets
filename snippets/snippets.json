{
  "arrowFunc": {
    "body": [
      "const $1 = ($2) => $3"
    ],
    "description": "single-line function",
    "prefix": "_arrow"
  },
  "braces": {
    "body": [
      "{$1} $2"
    ],
    "description": "braces",
    "prefix": "braces"
  },
  "catch": {
    "body": [
      ".catch(err => console.log(err, \"error\"))"
    ],
    "description": "Promise catch",
    "prefix": "catch"
  },
  "component": {
    "body": [
      "export const $1 = ({$2, ...props}) => {",
      "  $4",
      "  return (",
      "   $3",
      " )",
      "}"
    ],
    "description": "React Component",
    "prefix": "funcComp"
  },
  "consoleLog": {
    "body": [
      "console.log('$1', $2);"
    ],
    "description": "Log output to console",
    "prefix": "consoleLog"
  },
  "import": {
    "body": [
      "import {$1} from '$2';"
    ],
    "description": "import statement",
    "prefix": "import"
  },
  "parenthesis": {
    "body": [
      "($1) $2"
    ],
    "description": "parens",
    "prefix": "parens"
  },
  "useEffect": {
    "body": [
      "useEffect(() => {",
      "$1",
      "}, []);"
    ],
    "description": "useState",
    "prefix": "useEffect"
  },
  "useState": {
    "body": [
      "const [$1, $2] = useState($3);"
    ],
    "description": "useState",
    "prefix": "useState"
  }
}
