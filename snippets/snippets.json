{
  "console.log": {
    "prefix": "cl",
    "body": [
      "console.log('$1');"
    ],
    "description": "Log output to console"
  },
  "arrow function": {
    "prefix": "f",
    "body": [
      "($1) => $2"
    ],
    "description": "single-line function"
  },
  "braces": {
    "prefix": "b",
    "body": [
      "{",
        "$1",
      "}"
    ],
    "description": "multi-line braces"
  },
  "catch": {
    "prefix": "ct",
    "body": [
      ".catch(err => console.log(err, \"error\"))"
    ],
    "description": "Promise catch"
  }
}
